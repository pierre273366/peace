{
  "expo": {
    "name": "Peace",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/peacelogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/peacelogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5f5f7"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/peacelogo.png",
        "backgroundColor": "rgb(247, 247, 255)"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/peacelogo.png"
    },
    "extra": {
      "eas": {
        "projectId": "181bbbe8-72b7-4a10-8f07-080f6692f57d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/181bbbe8-72b7-4a10-8f07-080f6692f57d"
    }
  }
}
